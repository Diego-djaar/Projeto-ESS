{
  "name": "frontend-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "testing": "vite --mode testing",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run --mode testing",
    "test:coverage": "npm test -- --coverage",
    "test:show-coverage": "open ./coverage/index.html",
    "cy:open-e2e": "cypress open open --e2e --browser electron",
    "cy:run-e2e": "cypress run --e2e",
    "cy:e2e-interactive": "start-server-and-test testing http-get://localhost:3123 cy:open-e2e",
    "cy:e2e-headless": "start-server-and-test testing http-get://localhost:3123 cy:run-e2e",
    "cy:show-coverage": "open ./cypress-coverage/lcov-report/index.html"
  },
  "dependencies": {
